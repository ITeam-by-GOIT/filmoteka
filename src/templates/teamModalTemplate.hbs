<ul class='team-list list'>
  {{#each this}}
    <li class='team-list-item'>
      <div class='image-wrapper'>
        <img class='about-list-img' src='{{img}}' alt='Фото {{name}} {{lastName}}' />
      </div>
      <div class='team-list-text-container'>
        <h3 class='team-list-title'>{{name}} {{lastName}}</h3>
        <ul class='social-list list'>
          <li class='social-list-item'>
            <a href='{{githubLink}}' class='social-list-link'>
              <svg class='social-list-icon' width='20' height='20'>
                <use href='./sprite.svg#icon-github'></use>
              </svg>
            </a>
          </li>
          <li class='social-list-item'>
            <a href='{{linkedInLink}}' class='social-list-link'>
              <svg class='social-list-icon' width='20' height='20'>
                <use href='./sprite.svg#icon-linkedin'></use>
              </svg>
            </a>
          </li>
        </ul>
      </div>
    </li>
  {{/each}}
</ul>